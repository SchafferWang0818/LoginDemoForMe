<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_tel"
    style="@style/style_layout_account"
    android:layout_marginTop="70px"
    android:layout_below="@id/login_brand"
    tools:showIn="@layout/activity_login">

    <ImageView
        android:id="@+id/login_iv_shoujihao"
        style="@style/style_layout_login_iv"
        android:src="@drawable/shoujihao" />

    <View
        android:id="@+id/login_view_fenge1"
        style="@style/style_layout_fengexian" />

    <EditText
        android:id="@+id/login_shoujihao"
        style="@style/style_layout_edt"
        android:digits="0123456789"
        android:maxLength="11"
        android:singleLine="true"
        android:hint="@string/str_write_account" />
</LinearLayout>