<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rectangle_dialog">
    <!--    android:layout_centerInParent="true"
        android:layout_marginTop="338px"
        android:layout_gravity="center_horizontal"-->
    <ImageView
        android:id="@+id/dialog_weizhuce_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60px"
        android:src="@drawable/shoujiweizhuce" />

    <TextView
        android:id="@+id/dialog_register_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/dialog_weizhuce_iv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40px"
        android:text="手机号尚未注册,请返回注册"
        android:textSize="28px" />

    <Button

        android:id="@+id/dialog_register_btn"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_below="@id/dialog_register_tv"
        android:layout_marginTop="44px"
        android:background="@drawable/rectangle_dialog_btn"
        android:text="确定"
        android:textColor="#ffffff" />

</RelativeLayout>